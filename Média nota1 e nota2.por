algoritmo
 //Obter a média após o resultado de duas notas
 declare a,b, resul numerico
 escreva "Digite a nota n1"
 leia a 
 escreva "Digite a nota n2"
 leia b
 resul <- (a + b) /2
 se resul >= 6 //Teste verdadeiro ou falso 
 entao escreva "Aluno aprovado. Nota ", resul
 senao  escreva "Aluno reprovado. Nota ", resul 
fim_algoritmo
